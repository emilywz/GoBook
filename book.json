{
    "plugins": [
		"-lunr"	,
		"-search",
		"search-pro",
		"pageview-count",
		"tbfed-pagefooter", 
		"expandable-chapters",
		"editlink",
		"katex",
		"splitter",
		"github",
		"code",
		"livereload",
		"page-treeview",
		"lightbox",
		"advanced-emoji",
		"anchor-navigation-ex",
		"hide-element",
		"-sharing",
		"theme-fexa"
    ],
	"variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"http://localhost:4000",
                    "target":"_blank",
                    "name": "知识记录"
                }
            ]
        }
    },
    "pluginsConfig": {  
		"tbfed-pagefooter": {
			"copyright": "Copyright &#169; Emily",
			"modify_label": "修订时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"editlink": {
			"base": "https://github.com/emilywz/GoBook/tree/master/",
			"label": "Edit This Page",
			"multilingual": false
		},
		"github":{
			"url":"https://github.com/emilywz"
		},
		"search-pro": {
			"cutWordLib": "nodejieba",
			"defineWord" : ["Gitbook Use"]
		},
		"hide-element":{
			"element":[".gitbook-link"]
		},
		"theme-fexa":{
			"search-placeholder":"输入关键字搜索"
		},
		"page-treeview": {
            "copyright": "Copyright &#169; Emily",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
		},
		"anchor-navigation-ex": {
			"showLevel": false,
			"showGoTop": true,
			"associatedWithSummary": true,
			"printLog": true,
			"multipleH1": true,
			"mode": "float",
			"float": {
				"showLevelIcon": false,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			}
		}
	},
    "description": "emily go study",
    "author": "emily",
    "language": "zh-hans",
	"root": "./",
	"label": {
        "en": "Edit This Page",
        "zh-cn": "编辑本页"
    }
}
